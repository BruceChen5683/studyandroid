<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="www.battlecall.tk.basedemo.activity.StartupModeActivityStandard">

    <Button
        android:id="@+id/startSelf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="startSelf"/>

    <Button
        android:id="@+id/startTop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="startTop"/>

    <Button
        android:id="@+id/startsingleTask"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="startsingleTask"/>
</LinearLayout>
